<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="beanWantingExplicitName" class="org.kubek2k.springockito.jira011.spy.BeanInjectedWith">
        <property name="firstSpiedBean" ref="firstSpiedBeanArbitraryName"/>
    </bean>

    <bean id="firstSpiedBeanArbitraryName" class="org.kubek2k.springockito.jira011.spy.FirstSpiedBean"/>

</beans>
