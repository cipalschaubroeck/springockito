<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="beanToBeInjected" class="org.kubek2k.springockito.jira011.spy.BeanInjectedWith">
        <property name="firstSpiedBean" ref="firstBean"/>
        <property name="secondSpiedBean" ref="secondBean"/>
    </bean>

    <bean name="firstBean" class="org.kubek2k.springockito.jira011.spy.FirstSpiedBean"/>
    <bean name="secondBean" class="org.kubek2k.springockito.jira011.spy.SecondSpiedBean"/>

    <bean name="firstBeanUnwantedName" class="org.kubek2k.springockito.jira011.spy.FirstSpiedBean"/>
    <bean name="secondBeanUnwantedName" class="org.kubek2k.springockito.jira011.spy.SecondSpiedBean"/>
</beans>
